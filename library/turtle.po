msgstr "\"fastest\":  0"

#: ../../library/turtle.rst:598
msgid "\"fast\":  10"
msgstr "\"fast\":  10"

#: ../../library/turtle.rst:599
msgid "\"normal\":  6"
msgstr "\"normal\":  6"

#: ../../library/turtle.rst:600
msgid "\"slow\":  3"
msgstr "\"slow\":  3"

#: ../../library/turtle.rst:601
msgid "\"slowest\":  1"
msgstr "\"slowest\":  1"

