msgid "System"
msgstr ""

#: ../../library/sys.rst:1364
msgid "``platform`` value"
msgstr ""

#: ../../library/sys.rst:1366
msgid "AIX"
msgstr ""

#: ../../library/sys.rst:1366
msgid "``'aix'``"
msgstr "``'aix'``"

#: ../../library/sys.rst:1367
msgid "Emscripten"
msgstr ""

#: ../../library/sys.rst:1367
msgid "``'emscripten'``"
msgstr ""

#: ../../library/sys.rst:1368
msgid "Linux"
msgstr ""

#: ../../library/sys.rst:1368
msgid "``'linux'``"
msgstr "``'linux'``"

#: ../../library/sys.rst:1369
msgid "WASI"
msgstr ""

#: ../../library/sys.rst:1369
msgid "``'wasi'``"
msgstr ""

#: ../../library/sys.rst:1370
msgid "Windows"
msgstr "Windows"

#: ../../library/sys.rst:1370
msgid "``'win32'``"
