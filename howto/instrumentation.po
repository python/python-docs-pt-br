msgstr "$ yum install systemtap-sdt-devel"

#: ../../howto/instrumentation.rst:44
msgid "or::"
msgstr "ou::"

#: ../../howto/instrumentation.rst:46
msgid "$ sudo apt-get install systemtap-sdt-dev"
msgstr "$ sudo apt-get install systemtap-sdt-dev"

