"    return \"azul\"\n"
"\n"
"class FilhaBoa(Base):\n"
"  @override  # ok: substitui Base.obtém_cor\n"
"  def obtém_cor(self) -> str:\n"
"    return \"amarelo\"\n"
"\n"
"class FilhaRuim(Base):\n"
"  @override  # erro de verificador de tipo: não substitui Base.obtém_cor\n"
"  def obtém_cores(self) -> str:\n"
"    return \"vermelho\""

